{
  "name": "react-seed",
  "version": "1.0.0",
  "description": "React Seed Project",
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "./build/bundle.js",
  "scripts": {
    "test": "./node_modules/.bin/karma start",
    "dev": "DEBUG=true node server.dev.js",
    "dev:playground": "PLAYGROUND=true DEBUG=true node server.dev.js",
    "prod": "NODE_ENV=production node server.dev.js",
    "prod-min": "COMPRESS=true NODE_ENV=production node server.dev.js",
    "mock-server": "node_modules/.bin/nodemon test/mock-server.js",
    "dev-mock": "MOCK_SERVER=true npm run dev",
    "proxy": "devd -p 9000 -A 0.0.0.0 /app/=http://localhost:8080 /api/=http://localhost:7003 /static/=./static /login/=./login",
    "tmux": "tools/scripts/tmux-dev-session",
    "scaffold": "./node_modules/.bin/plop",
    "clean": "rm -rf build && rm -rf release",
    "build:dev": "npm run clean && ./node_modules/.bin/webpack --config webpack.config.base.js lib/main.js build/bundle.js",
    "build:prod": "npm run clean && COMPRESS=true NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.base.js lib/main.js build/bundle.js",
    "start": "./node_modules/.bin/concurrent -st --timestamp-format \"HH:mm:ss\" \"npm run dev\" \"npm run mock-server\" \"npm run proxy\""
  },
  "author": "Grayfox",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.0",
    "babel-core": "^6.1.21",
    "babel-loader": "^6.0.1",
